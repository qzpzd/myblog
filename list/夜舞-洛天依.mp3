<script type="text/javascript">

    $(function () {
        $('#s-btn').click(function () {

            let keyword = $.trim($('#s-input').val());
            if (keyword.length <= 0) {
                input_err('请输入搜索关键词~');
                return false;
            }

            if (keyword.length > 30) {
                input_err('关键词字数不能超过30位~');
                return false;
            }

            $('#s-btn').attr("disabled", true);
            $('#search').hide();
            $('#loading').show();

            $.ajax({
                type: "POST",
                url: "api/s",
                data: {
                    "keyword": keyword
                },
                success: function (result) {
                    window.location.href = '/s/' + keyword;
                },
                error: function (e) {
                    input_err('请求失败,请稍后再试~');
                },
                complete: function () {
                    $('#s-btn').attr("disabled", false);
                    $('#search').show();
                    $('#loading').hide();
                }
            });

        });

        // 输入框获取焦点清空错误提示
        $('#s-input').focus(function () {
            $(this).removeClass('is-invalid');
            $(".search-img").animate({"height": "24px", "top": "-24px"}, 1000);
        }).blur(function () {
            // 失去焦点隐藏图片
            $(".search-img").animate({"height": "0", "top": "0"}, 1000);
        }).keydown(function (event) {
            //键盘回车事件
            if (event.keyCode == 13) {
                $("#s-btn").click();
            } else {
                $(this).removeClass('is-invalid');
            }

        });

        // 错误提示函数
        function input_err(msg) {
            $('#invalid-msg').text(msg);
            $('#s-input').focus().removeClass('border-secondary').addClass('is-invalid');
        }

    });
</script>

