<script type="text/javascript">
    $(function () {

        $('#resetFormBtn').click(function () {
            $('#feedbackContent').val('');
        });

        $('#feedbackType').change(function () {
            if ($(this).val() == "bug") {
                $('#link-group').show();
                $('#link').focus();
            } else {
                $('#link-group').hide();
            }
        });


        $('#submitFormBtn').click(function () {
            let fkjy_type = $.trim($('#feedbackType').val());
            if (!fkjy_type) {
                alert('请先选择反馈类型');
                $('#feedbackType').focus();
                return false;
            }

            let link = $.trim($('#link').val());
            if (fkjy_type == "bug" && link.length < 1) {
                alert('请输入报错的链接');
                $('#link').focus();
                return false;
            }

            let fkjy_content = $.trim($('#feedbackContent').val());
            if (fkjy_content.length < 1) {
                alert('请输入建议内容~');
                $('#feedbackContent').focus();
                return false;
            }

            let fkjy_contact = $.trim($('#feedbackContact').val());
            if (fkjy_contact.length < 1) {
                alert('请输入联系方式~');
                $('#feedbackContact').focus();
                return false;
            }
            if (fkjy_contact.indexOf('@') == -1) {
                alert('联系邮箱格式不正确~');
                $('#feedbackContact').focus();
                return false;
            }

            $.ajax({
                type: "POST",
                url: "/api/fkjy",
                data: {
                    "content": "[" + fkjy_type + "] " + fkjy_content,
                    "contact": fkjy_contact,
                    "link": link
                },
                success: function (result) {
                    alert(result);
                    $('#feedbackModal').modal('hide')
                },
                error: function (e) {
                    if (e.status == 429) {
                        alert('提交太频繁，请稍后再试！')
                        return;
                    }
                    alert('【提交出错】 ' + e.responseText)
                }
            });
        });

        $('#swhz').click(function () {
            layer.alert("合作联系邮箱：cnbgm@outlook.com");
